package lab7.Buddies;

// represents a list of Person's buddies
interface ILoBuddy {
  // is the given person in this list
  boolean contains(Person p);
  boolean hasDirectBuddy(Person p);
  
  int countCommonBuddies(ILoBuddy other);

  ILoBuddy addIfNotExist(Person other);
  ILoBuddy addAllIfNotExist(ILoBuddy other);
  ILoBuddy addBuddiesOfAllToList(ILoBuddy buddiesToList);
}
